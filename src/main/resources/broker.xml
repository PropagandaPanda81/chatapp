<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns="urn:activemq:core"> 
    <name>embedded-broker</name> 
    <persistence-enabled>false</persistence-enabled>
    <security-enabled>false</security-enabled>

    <addresses>
        <address name="testQueue">
            <anycast>
                <queue name="testQueue"/>
            </anycast>
        </address>
        <address name="sendQueue">
            <anycast>
                <queue name="sendQueue"/>
            </anycast>
        </address>
        <address name="receiveQueue">
            <anycast>
                <queue name="receiveQueue"/>
            </anycast>
        </address>
        <address name="chatQueue">
            <anycast>
                <queue name="chatQueue"/>
            </anycast>
        </address>
    </addresses>
</configuration>
